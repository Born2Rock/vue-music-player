{
  "name": "vue-vite-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:dev": "concurrently \"npm run dev-server:start\" \"npm run start\"",
    "preview": "vite preview",
    "build": "vite build",
    "prettier:check": "npx prettier --check src/**/*.{js,ts,tsx,json,vue}",
    "prettier:write": "npx prettier --write src/**/*.{js,ts,tsx,json,vue}",
    "lint:ts": "eslint \"**/*.{tsx,tsx,vue}\"",
    "lint:ts:quiet": "eslint \"**/*.{tsx,tsx,vue}\" --quiet",
    "lint:ts:fix": "eslint \"**/*.{tsx,tsx,vue}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "dev-server:start": "json-graphql-server ./json-graphql-server/db.cjs  --p 8000",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "npx cypress open"
  },
  "dependencies": {
    "@apollo/client": "^3.7.4",
    "@vue/apollo-composable": "^4.0.0-beta.1",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.0.29",
    "sass": "^1.57.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@pinia/testing": "^0.0.15",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.15",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.10",
    "@vue/vue3-jest": "^29.2.2",
    "babel-jest": "^29.4.2",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "cross-fetch": "^3.1.5",
    "cypress": "^12.5.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-andrews": "^0.0.18",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "jest-transform-stub": "^2.0.0",
    "json-graphql-server": "^2.3.2",
    "loki": "^0.31.0",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.3",
    "stylelint": "^14.16.1",
    "stylelint-config-hudochenkov": "^8.0.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^6.0.1",
    "ts-jest": "^29.0.5",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-svg-loader": "^4.0.0",
    "vite-tsconfig-paths": "^4.0.3",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.0.11"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
